{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",
    "strict": false,
    "allowJs": true,
    "outDir": "./.tsbuild/",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
